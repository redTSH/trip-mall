<div class="col">
    <a href="">
        <div class="big-grid">
            <img src="{{one.url}}" alt="">
            <div class="info">
                <p>{{one.title}}</p>
                <div class="downside">
                    <p class="tag">{{one.type}}</p>
                    <p class="price"><strong>{{one.price}}</strong>元起</p>
                </div>
            </div>
        </div>
    </a>
    <a href="">
        <div class="nor-grid">
            <div class="picbox">
                <img src="{{items[0].url}}" alt="">
            </div>
            <div class="wordsbox">{{items[0].title}}</div>
        </div>
    </a>
</div>
<div class="col">
    <a href="">
        <div class="nor-grid">
            <div class="picbox">
                <img src="{{items[1].url}}" alt="">
            </div>
            <div class="wordsbox">{{items[1].title}}</div>
        </div>
    </a>
    <a href="">
        <div class="nor-grid">
