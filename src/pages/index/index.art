    <!-- 最世界·深度旅行 -->
    <!-- 复制机酒自由行模块更改类名,达到复用样式效果 -->
    <section class="zsj title-part common-part">
        <div class="center_wrap">
            {{include './components/zsj/zsj.art'}}
