    <!-- 复制机酒自由行模块更改类名,达到复用样式效果 -->
    <section class="gty title-part common-part">
        <div class="center_wrap">
            {{include './components/gty/gty.art'}}
        </div>
    </section>
    <footer>
        <div class="footer-part1">
            <div class="center_wrap">
                {{include '../../components/footer-nav/footer-nav.art'}}
            </div>
        </div>
        <div class="footer-part2">
            <div class="center_wrap">
                {{include '../../components/bottom-bar/bottom-bar.art'}}
            </div>
        </div>
    </footer>
    <!-- 返回顶部按钮 -->
    {{include '../../components/backtotop/backtotop.art'}}

